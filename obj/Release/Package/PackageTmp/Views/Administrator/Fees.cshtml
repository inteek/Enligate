        <div class="row">
            <div class="col-xs-12 paddingLRZero">
                <div class="col-xs-12">
                    @Html.Partial("Administrator/_FeesForm", new TarifasViewModel())
                </div>
            </div>
        </div>

        <div class="row mSpaceMt20 mSpaceMb20">
            <div class="col-xs-12">
