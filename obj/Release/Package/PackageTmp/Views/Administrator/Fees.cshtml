        fnFeesGVPerformCallback();
    });

    function fnFeesGVPerformCallback(s, e) {
        if (typeof gvFeesGrid.InCallback === "function") {
            //if (!gvFeesGrid.InCallback())
            gvFeesGrid.PerformCallback();
        }
    }

</script>

<!-- Content -->
<div id="ADMINISTRADOR_Pagos" class="row">
